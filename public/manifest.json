{
  "manifest_version": 2,
  "name": "Demotivational New Tab",
  "version": "1.0.0",
  "description": "Get demotivated evey time you open a new tab!",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "32": "icons/icon32.png",
    "16": "icons/icon16.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "Browser Extension"
  },
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "permissions": ["activeTab", "tabs", "<all_urls>", "storage"]
}
